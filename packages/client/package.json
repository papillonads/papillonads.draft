{
  "name": "papillonads.nl-client",
  "version": "0.0.0",
  "description": "Papillon Ads Holland Client",
  "main": "index.js",
  "license": "MIT",
  "files": [
    "build"
  ],
  "keywords": [
    "papillon ads holland client"
  ],
  "author": {
    "name": "MTS",
    "url": "https://github.com/mts"
  },
  "scripts": {
    "start": "nodemon --watch webpack.settings.js --watch webpack.common.js --watch webpack.dev.js --exec \"webpack-dev-server --config webpack.dev.js\"",
    "serve": "echo \"No serve specified\" && exit 0",
    "build": "webpack --config webpack.prod.js --progress --hide-modules",
    "release": "npm publish",
    "uninstall-packages": "rm -rf node_modules package-lock.json",
    "install-packages": "npm install",
    "reinstall-packages": "npm-run-all uninstall-packages install-packages",
    "npm-update": "npm update",
    "test": "jest -u --config jest.config.json --no-cache --silent --runInBand",
    "test:watchChanged:silent": "jest -u --config jest.config.json --watch --onlyChanged --verbose",
    "test:coverage": "jest --config jest.config.json --coverage --silent",
    "lint:format": "npm-run-all lint format",
    "lint": "npm-run-all lint:js:jsx lint:css:scss",
    "lint:js:jsx": "eslint --ext .js --ext .jsx src/ --fix",
    "lint:css:scss": "stylelint \"src/**/*.{css,scss}\" --syntax=scss --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,scss}\"",
    "ci": "npm-run-all lint:format build test"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR"
    ],
    "legacyBrowsers": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR"
    ],
    "modernBrowsers": [
      "last 2 Chrome versions",
      "not Chrome < 60",
      "last 2 Safari versions",
      "not Safari < 10.1",
      "last 2 iOS versions",
      "not iOS < 10.3",
      "last 2 Firefox versions",
      "not Firefox < 54",
      "last 2 Edge versions",
      "not Edge < 15"
    ]
  },
  "devDependencies": {},
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "@babel/runtime": "^7.6.3",
    "@primer/css": "^13.1.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "classnames": "^2.2.6",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "papillon-alerts": "^1.0.51",
    "papillon-avatars": "^1.0.51",
    "papillon-blankslate": "^1.0.51",
    "papillon-breadcrumb": "^1.0.51",
    "papillon-buttons": "^1.0.51",
    "papillon-labels": "^1.0.51",
    "papillon-layout": "^1.0.51",
    "papillon-navigation": "^1.0.51",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-apollo": "^3.1.3",
    "react-autocomplete": "^1.8.1",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.0.8",
    "uuid": "^3.3.3"
  }
}
