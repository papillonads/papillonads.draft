{
  "testRegex": ".test\\.js$",
  "roots": ["<rootDir>"],
  "transform": {
    "^.+\\.jsx?$": "babel-jest"
  },
  "collectCoverageFrom": ["packages/**/*.js", "packages/**/*.jsx"],
  "coveragePathIgnorePatterns": ["node_modules", ".storybook", ".storybook-static", "asset", ".test.js", ".story.js", ".render.js"],
  "setupFilesAfterEnv": ["<rootDir>/jest.config.js"],
  "moduleNameMapper": {
    "\\.(css|less|scss)$": "identity-obj-proxy"
  },
  "moduleFileExtensions": ["js", "jsx"]
}
