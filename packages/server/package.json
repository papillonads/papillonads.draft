{
  "name": "papillonads.nl-server",
  "version": "0.0.0",
  "description": "Papillon Ads Holland Server",
  "main": "index.js",
  "license": "MIT",
  "files": [
    "src"
  ],
  "keywords": [
    "papillon ads holland server"
  ],
  "author": {
    "name": "MTS",
    "url": "https://github.com/mts"
  },
  "scripts": {
    "start": "nodemon src/index.js",
    "serve": "npm start",
    "prebuild": "rimraf build",
    "build": "babel src --out-dir build --copy-files",
    "release": "npm publish",
    "uninstall-packages": "rm -rf node_modules package-lock.json",
    "install-packages": "npm install",
    "reinstall-packages": "npm-run-all uninstall-packages install-packages",
    "npm-update": "npm update",
    "test": "echo \"No test specified\" && exit 0",
    "test:coverage": "echo \"No test coverage specified\" && exit 0",
    "lint:format": "npm-run-all lint format",
    "lint": "npm-run-all lint:js:jsx",
    "lint:js:jsx": "eslint --ext .js --ext .jsx src/ --fix",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "ci": "npm-run-all lint:format build test"
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "apollo-datasource-rest": "^0.3.2",
    "apollo-server": "^2.4.8",
    "graphql": "^14.2.1",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3"
  }
}
