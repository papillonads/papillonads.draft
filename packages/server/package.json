{
  "name": "papillon-explorer-server",
  "version": "0.0.8",
  "description": "Papillon Explorer Server",
  "main": "index.js",
  "license": "MIT",
  "files": [
    "src"
  ],
  "keywords": [
    "papillon explorer server"
  ],
  "scripts": {
    "start": "nodemon src/index.js",
    "serve": "npm start",
    "prebuild": "rimraf build",
    "build": "babel src --out-dir build --copy-files",
    "release": "npm publish",
    "uninstall-packages": "rm -rf node_modules package-lock.json",
    "install-packages": "npm install",
    "reinstall-packages": "npm-run-all uninstall-packages install-packages",
    "test": "echo \"No test specified\" && exit 0",
    "lint": "npm-run-all lint:js",
    "lint:js": "eslint src/*.js --fix",
    "lint:staged": "lint-staged",
    "ci": "npm-run-all lint build test"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "apollo-datasource-rest": "^0.2.1",
    "apollo-server": "^2.4.0",
    "graphql": "^14.1.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3"
  }
}
